{
  "name": "@polym/react-props",
  "description": "Utility for defining props for React components",
  "version": "1.0.1",
  "keywords": [
    "react",
    "props",
    "ts",
    "tsx",
    "typescript"
  ],
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "tomixy"
  },
  "types": "./types/index.d.ts",
  "files": [
    "/dist",
    "/types"
  ],
  "main": "./dist/polym-react-props.umd.js",
  "module": "./dist/polym-react-props.es.js",
  "exports": {
    ".": {
      "import": "./dist/polym-react-props.es.js",
      "require": "./dist/polym-react-props.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "watch": "vite build --watch",
    "prepare": "yarn build",
    "preview": "vite preview",
    "lint": "eslint --ext .tsx,.ts src/",
    "lintfix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.+(js|json|yml|ts|tsx)\""
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@types/lodash": "^4.14.185",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.8",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
